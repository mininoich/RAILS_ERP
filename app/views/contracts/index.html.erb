<h1>Contract</h1>
<table class="table table-bordered table-condensed">
	<thead>
			<td>Etudiant</td>
			<td>Entreprise</td>
			<td>Type de contrat</td>
			<td>Date de d√©but</td>
			<td>Date de fin</td>
			<td>Action</td>
	</thead>
<% @contracts.each do |c| %>
	<tr>
		<td><%= c.students %></td>
		<td><%= c.companies %></td>
		<td><%= c.contract_types %></td>
		<td><%= c.date_debut %></td>
		<td><%= c.date_fin %></td>
		<td>
			<%= link_to "Show", c %>
			<% if admin_signed_in? %>
				<%= link_to "Edit", [:edit, c] %>
				<%= link_to 'Delete', c, :confirm => 'Are you sure ?', :method => :delete %>
			<% end %>
		</td>
	</tr>
<% end %>
</table>
<%= link_to "Add", :action => 'new' %>
